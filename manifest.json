{
  "manifest_version": 2,
  "name": "Should Do",
  "version": "1.0",
  "description": "This extension is can be used as a advanced todo list",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./main.js"],
      "css": ["style.css"],
      "run_at": "document_idle"
    }
  ]
}
